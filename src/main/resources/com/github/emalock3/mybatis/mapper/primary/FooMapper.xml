<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.emalock3.mybatis.mapper.primary.FooMapper">
  <select id="findOne" resultType="com.github.emalock3.mybatis.mapper.domain.Foo">
    select * from Foo where id = #{id}
  </select>
  <select id="findAll" resultType="com.github.emalock3.mybatis.mapper.domain.Foo">
    select * from Foo
  </select>
</mapper>
